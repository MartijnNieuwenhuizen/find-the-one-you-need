<li class="knolage-areas--item area area-project">
  <div class="area--title-container">
    <h3 class="area--title">Projecten</h3>
  </div>
  <ul class="projects">
    <% person.knolage.projects.forEach(project => { %>
      <%- include ./project.ejs %>
    <% }) %>
  </ul>
  <p><%- person.name.first %></p>
  <% if (person.knolage.projects.length > 1) { %>
    <a href="#">Bekijk alle projecten waar <%- person.name.first %> aan heeft gewerkt</a>
  <% } %>
</li>
